<!DOCTYPE html>
<html lang="ja">

  <head>
    <meta charset="UTF-8">
    <title>論破.net</title>
    <link rel="stylesheet" type="text/css" href="./stylesheets/style.css">
    <script type="text/javascript" src="/socket.io/socket.io.js"></script>
    <script type="text/javascript" src="./javascripts/rtc.js"></script>
  </head>

  <body>
    <div class="container">
      <h1 class="title"><a href="./">論破.net</a></h1>

      <section class="play_top">
        <div class="play_input">
          <p id="play_input_roomID"><%= roomID %></p>
          <p id="play_input_user_name"><%= user_name %></p>
          <p id="play_input_type"><%= item_type %></p>
        </div>
        <div class="play_theme">
          <h1><%= theme %></h1>
        </div>
      </section>

      <section class="play_view">
        <div class="play_video">
          <video id="myvideo" playsinline autoplay muted></video>
        </div>
        <h1 class="play_vs">VS</h1>
        <div class="play_video">
          <video id="remote_video" playsinline autoplay></video>
        </div>
      </section>

      <section class="play_log">
        <ul id="play_room_log">
          <!-- log -->
        </ul>
      </section>

    </div>
  </body>
</html>
